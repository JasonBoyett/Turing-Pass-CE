{
  "manifest_version": 3,
  "name": "Turing Pass",
  "description": "One password to rule them all",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "cookies",
    "declarativeContent"
  ],
  "author": "Jason Boyett",
  "content_scripts":[
    {
    "matches": [
        "http://*/*login*",
        "https://*/*login*",
        "http://*/*signin*",
        "https://*/*signin*",
        "http://*/*auth*",
        "https://*/*auth*",
        "http://*/*signup*",
        "https://*/*signup*",
        "http://*/*register*",
        "https://*/*register*",
        "<all_urls>"
      ],
    "js": ["main.js"] 
    }
  ]
}

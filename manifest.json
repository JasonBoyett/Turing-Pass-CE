{
  "manifest_version": 3,
  "name": "Turing Pass",
  "description": "One password to rule them all",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "cookies",
    "declarativeContent"
  ],
  "author": "Jason Boyett",
  "content_scripts":[
    {
    "matches": ["<all_urls>"],
    "js": ["main.js"] 
    }
  ]
}
